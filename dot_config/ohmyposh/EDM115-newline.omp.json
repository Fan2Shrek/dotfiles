{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "text",
          "template": "\n",
          "foreground": "#282a36",
          "background": "#282a36"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "leading_diamond": "\ue0b6",
          "template": "{{ .UserName }}", "foreground": "#f8f8f2",
          "background": "#282a36",
          "type": "session",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "\udb85\udc0b",
          "foreground": "#ff5555",
          "powerline_symbol": "\ue0b0",
          "background": "#282a36",
          "type": "root",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "{{ .Icon }}  ",
          "foreground": "#f8f8f2",
          "powerline_symbol": "\ue0b0",
          "background": "#282a36",
          "type": "os",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none",
            "style": "folder"
          },
          "trailing_diamond": "\ue0b4",
          "template": " \udb80\ude56 {{ path .Path .Location }}",
          "foreground": "#282a36",
          "background": "#cccccc",
          "type": "path",
          "style": "diamond"
        }
      ]
    }
  ],
  "version": 3,
  "patch_pwsh_bleed": true,
  "final_space": true
}
